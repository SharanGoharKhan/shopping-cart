{
  "expo": {
    "version": "2.0.5",
    "name": "Ecommero",
    "description": " Ecommero is a starter kit ecommerce UI theme built in React Native using Expo for IOS and Android. It's made keeping good aesthetics in mind as well keeping the best coding practices in mind. Its fully customisable to easily help you in your next ecommerce project. https://market.nativebase.io/view/react-native-e-commero-ui-theme",
    "slug": "ecommero-app",
    "privacy": "public",
    "androidStatusBar": {
      "backgroundColor": "#000"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon-ios.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecommero.app",
      "usesAppleSignIn": true
    },
    "notification": {
      "iosDisplayInForeground": true,
      "color":"#D0AD8A",
      "icon": "./assets/image.png",
      "androidMode": "default",
       "androidCollapsedTitle": "Ecommero"
    },
    "android": {
      "versionCode": 6,
      "package": "com.ecommero.app",
      "permissions": [],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    }
  }
}
